{
  "name": "tbi/ynotradio",
  "description": "A WordPress install built with varying vagrant vagrants.",
  "license": "proprietary",
  "authors": [
    {
      "name": "Dan Gautsch",
      "email": "dan.gautsch@thinkcompany.com",
      "homepage": "http://www.thinkcompany.com/",
      "role": "Development Lead"
    },
    {
      "name": "Dan Grebb",
      "email": "dan.grebb@thinkcompany.com",
      "homepage": "http://www.thinkcompany.com/",
      "role": "Developer"
    }
  ],
  "type": "project",
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "secure-http": false
  },
  "require": {
    "johnpbloch/wordpress": "^4.7"
  },
  "require-dev": {
    "wpackagist-plugin/log-deprecated-notices": "*"
  },
  "extra": {
    "wordpress-install-dir": "htdocs",
    "installer-paths": {
      "htdocs/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "htdocs/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  }
}
